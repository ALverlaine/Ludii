(metadata
  
  (info
      {
      }
  )
  
  (graphics {
      (board colour Phase0 (colour Hidden))
      (board colour OuterEdges (colour Black))
      (board colour InnerEdges (colour Black))
      (show Symbol "C" (sites {0})) 
      (show Symbol "D" (sites {1})) 
  })
  
)