(game "Iterated Prisoners Dilemma"
	(players 2)
    (mode Simultaneous)  
	(equipment {
		(board (rectangle 1 2))
	})
	(rules
		(play 
			(or 
				(move Select (from (sites Board)) P1)
				(move Select (from (sites Board)) P2)
			)
		)
		(end (if (no Moves Next) (result Mover Win)))
	)
)

