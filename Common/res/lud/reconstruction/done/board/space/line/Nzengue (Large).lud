                                (not 
                                    (is Line 3 Orthogonal through:
                                        (site)
                                    )
                                )
                            )
                        )
                        
                        (forEach Piece)
                    )
                )
            )
            }
            
            (end 
                (forEach NonMover if:
                    (= 2 
                        (count Pieces Player)
                    )
                    
                    (result Player Loss)
                )
            )
        )
        
