                                    )
                                )
                                
                                (to if:
                                    (is Friend 
                                        (who at:
                                            (to)
                                        )
                                    )
                                )
                            )
                        )
                    )
                )
            )
            }
        )
        
        (rules 
            (start { 
                (place "Marker1" 
                    (union 
                        (sites Bottom)
                        
                        (expand 
                            (sites Row 2)
                        )
                    )
                )
                
                (place "Marker2" 
                    (union 
                        (sites Top)
                        
                        (expand 
                            (sites Row 6)
