            use:Vertex)
            
            (dice num:2)
            
            (piece "Disc" Each 
                (move 
                    (from 
                        (from)
                        level:
                        (level)
                    )
                    
                    (to 
                        (trackSite Move from:
                            (from)
                            steps:
                            (pips)
                        )
