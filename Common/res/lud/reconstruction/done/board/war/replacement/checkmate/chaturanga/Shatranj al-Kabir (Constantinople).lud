(define "CaptureToPiece"
    (apply 
        (if ("IsEnemyAt" (to))
            (remove (to))
        )
    ) 
)

(define "EnemyNotImmumned" 
    (and ("IsEnemyAt" (to))
        (not 
            (and { 
                ("IsPieceAt" "Pawn" Next (to))
                (is In (to) (sites Mover))
            })
        )
    )
)

(define "CaptureForwardDiagonal"
    (move
        Step 
        (directions {FR FL}) 
        (to 
            if:("EnemyNotImmumned") 
            (apply (remove (to)))
        )
    )
)

