(game "Example Recons"
    (players 2) 
    (equipment {  
        ("BackgammonBoard"
            {
            (track "Track1" {6 12..7 5..0 13..18 20..25 End} P1 directed:True)
            (track "Track2" {19 25..20 18..13 0..5 7..12 End} P2 directed:True)
            } 
        ) 
        (piece "Marker" Each [#])
        (dice d:6 num:3)
    })
    (rules
        (start [#]) // 15 pieces.
        (play 
            (do (roll)
                next:[#] // Two pieces on a space block an opponent from moving to that spot.
            )
        )
        (end [#])
    )
)